    pod 'TSNetworkMonitor', :git=>'http://gitlab.caiqr.com/ios_module/TSNetworkMonitor.git'
  pod 'SnapKit'
  pod 'Kingfisher'
#  pod 'TSRefresh'

  # Pods for TSCustomUIKit

  target 'TSCustomUIKitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TSCustomUIKitUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
